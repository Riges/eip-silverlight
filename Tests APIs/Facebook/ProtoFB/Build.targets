<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" DefaultTargets="Deploy">  <Import Project="C:\Program Files\MSBuild\MSBuildCommunityTasks\MSBuild.Community.Tasks.Targets" />  <Import Project="C:\Program Files\MSBuild\ExtensionPack\MSBuild.ExtensionPack.TASKS" />  <PropertyGroup>    <Configuration Condition="'$(Configuration)' == ''">Debug</Configuration>    <DeploymentType Condition="'$(DeploymentType)' == ''">proto</DeploymentType>    <Robocopy>C:\robocopy</Robocopy>  </PropertyGroup>  <Choose>    <When Condition=" '$(DeploymentType)'=='proto' ">      <PropertyGroup>        <DeploymentPath>C:\Inetpub\wwwroot\Proto</DeploymentPath>        <WebSite>EIP</WebSite>        <AppPool>DefaultAppPool</AppPool>      </PropertyGroup>    </When>        <Otherwise>      <PropertyGroup>      </PropertyGroup>    </Otherwise>  </Choose>      <Target Name="Deploy">    <Message Text="*********************************************************"/>    <Message Text="****************** Deploy TARGET ******************"/>    <!-- Deployment of Manage -->    <CallTarget Targets="DeployProto"></CallTarget>      </Target>  <Target Name="DeployProto">    <Message Text="*********************************************************"/>    <Message Text="****************** DeployProto Target ******************"/>    <Message Text="*** Deploy the files with Robocopy (miror mode) ***"/>      <!-- Robocopty Web -->    <Robocopy      Toolpath="$(Robocopy)"      SourceFolder="$(MSBuildProjectDirectory)\ProtoFB.Web"      DestinationFolder="$(DeploymentPath)"      Mirror="true"      Subdirectories="true"      ExcludeFolders=".svn"	  />      </Target>  </Project>